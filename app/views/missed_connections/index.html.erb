<p id="notice"><%= notice %></p>

<h1>Listing Missed Connections</h1>

<table>
  <thead>
    <tr>
      <th>Post</th>
      <th>Headline</th>
      <th>Post date</th>
      <th>Page url</th>
      <th>Borough</th>
      <th>Preference</th>
      <th>Place</th>
      <th>Body text</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>Created at</th>
      <th>Updated at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @missed_connections.each do |missed_connection| %>
      <tr>
        <td><%= missed_connection.post_id %></td>
        <td><%= missed_connection.headline %></td>
        <td><%= missed_connection.post_date %></td>
        <td><%= missed_connection.page_url %></td>
        <td><%= missed_connection.borough %></td>
        <td><%= missed_connection.preference %></td>
        <td><%= missed_connection.place %></td>
        <td><%= missed_connection.body_text %></td>
        <td><%= missed_connection.latitude %></td>
        <td><%= missed_connection.longitude %></td>
        <td><%= missed_connection.created_at %></td>
        <td><%= missed_connection.updated_at %></td>
        <td><%= link_to 'Show', missed_connection %></td>
        <td><%= link_to 'Edit', edit_missed_connection_path(missed_connection) %></td>
        <td><%= link_to 'Destroy', missed_connection, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Missed connection', new_missed_connection_path %>
